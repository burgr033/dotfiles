ZSH_THEME="bira"
export ZSH="$HOME/.oh-my-zsh"
plugins=(git virtualenvwrapper thefuck fast-syntax-highlighting zsh-autosuggestions fzf-tab autoupdate auto-ls archlinux)
ZSH_AUTOSUGGEST_STRATEGY=(history completion)
autoload -U compinit && compinit
source $ZSH/oh-my-zsh.sh
export GPG_TTY=$(tty)
export EDITOR=nvim
export VISUAL=nvim
export TERM=alacritty
alias vi="nvim"
alias vim="nvim"
alias cat="bat"
source /usr/share/fzf/key-bindings.zsh
source /usr/share/fzf/completion.zsh
